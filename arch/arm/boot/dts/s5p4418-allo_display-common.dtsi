	audio_en: regulator-fix@0 {
		compatible = "regulator-fixed";

		regulator-name = "AUDIO EN";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		regulator-boot-on;
		regulator-always-on;
	};

	iov: regulator-fix@1 {
		compatible = "regulator-fixed";

		regulator-name = "IOV";
	};

	ldoin: regulator-fix@2 {
		compatible = "regulator-fixed";

		regulator-name = "LDOIN";
	};

	dv: regulator-fix@3 {
		compatible = "regulator-fixed";

		regulator-name = "DV";
	};

	av: regulator-fix@4 {
		compatible = "regulator-fixed";

		regulator-name = "AV";
	};

	snd_null: snd_null@0 {
		#sound-dai-cells = <0>;
		compatible = "nexell,snd-null";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0 0>;
	};

	spdif_out: spdif-out {
		#sound-dai-cells = <0>;
		compatible = "linux,spdif-dit";
	};

    gpio_key: gpio_keys {
        compatible = "gpio-keys";
        #address-cells = <1>;
        #size-cells = <0>;
        pinctrl-names = "default";
        pinctrl-0 = <&key_volup &key_voldown &key_func1 &key_func2 &key_mute>;

        volup {
            label = "Volume Up";
            gpios = <&gpio_d 29 0>;
            linux,code = <KEY_VOLUMEUP>;
            gpio-key,wakeup;

        };

        voldown {
            label = "Volume-Down";
            gpios = <&gpio_d 30 0>;
            linux,code = <KEY_VOLUMEDOWN>;
        };

        func1 {
            label = "func-1";
            gpios = <&gpio_e 0 0>;
            linux,code = <KEY_F1>;
        };

        func2 {
            label = "func-2";
            gpios = <&gpio_e 1 0>;
            linux,code = <KEY_F2>;
        };

        mute {
            label = "mute";
            gpios = <&gpio_d 31 0>;
            linux,code = <KEY_MUTE>;
        };
    };

	snd_pcm_allo_display:snd_pcm_allo_display@1 {
		#sound-dai-cells = <0>;
		compatible = "nexell,snd-pcm-smart-voice";
		status = "okay";
	};

	snd_allo_display: snd_allo_display@0 {
		#sound-dai-cells = <0>;
		compatible = "nexell,snd-smart-voice,i2s,pl022";
		#address-cells = <1>;
		#size-cells = <0>;
		gpio_regs = < PHYS_BASE_GPIOA 0x1000
			PHYS_BASE_GPIOB 0x1000
			PHYS_BASE_GPIOC 0x1000
			PHYS_BASE_GPIOD 0x1000
			PHYS_BASE_GPIOE 0x1000
			PHYS_BASE_ALIVE 0x200>;

		clkgen_regs = <PHYS_BASE_CLK_15 0x1000
			PHYS_BASE_CLK_16 0x1000
			PHYS_BASE_CLK_17 0x1000>;

		ref {
			sound-dev = <&i2s_2 0>;
		};

		mic {
			ch@4 {
				sound-dev = <&snd_spi_2 0>;
				pin {
				spi-cs-gpio = <&gpio_c 16 0>;
				pdm-isrun-gpio = <&gpio_d 20 0>;
				pdm-lrck-gpio = <&gpio_b 26 0>;
				i2s-lrck-gpio = <&gpio_b 4 0>;
				pdm-nrst-gpio = <&gpio_d 16 0>;
				};
			};
		};
	};

	sound {
		compatible = "nexell,simple-audio-card";
		simple-audio-card,name = "s5p4418 allo_display Audio";
		simple-audio-card,widgets =
		    "Headphone", "Out Jack";
		simple-audio-card,routing =
		    "Out Jack", "HPL",
		    "Out Jack", "HPR";
		status = "okay";
		simple-audio-card,dai-link@0 {
			format = "i2s";
			cpu {
				sound-dai = <&i2s_0 0>;
			};

			codec {
				sound-dai = <&tas5756m>;
			};
		};

		simple-audio-card,dai-link@1 {
			format = "i2s";
			cpu {
				sound-dai = <&i2s_1 0>;
			};

			codec {
				sound-dai = <&snd_null>;
			};
		};

		simple-audio-card,dai-link@2 {
			format = "i2s";
			cpu {
				sound-dai = <&i2s_2 0>;
			};

			plat {
				sound-dai = <&snd_pcm_allo_display>;
			};

			codec {
				sound-dai = <&snd_allo_display>;
			};
		};

		simple-audio-card,dai-link@3 {
			format = "spdif";
			mclk-fs = <256>;
			cpu {
				sound-dai = <&spdif_tx 0>;
			};

			codec {
				sound-dai = <&spdif_out>;
			};
		};

		simple-audio-card,dai-link@4 {
			format = "spi";
			cpu {
				sound-dai = <&snd_spi_2 0>;
			};
			codec {
				sound-dai = <&snd_allo_display>;
			};
		};
	};
	bt_control {
		compatible = "broadcom,bcm434545bt";
		gpios = <&gpio_e 22 0>;   
		pinctrl-names = "default";
		pinctrl-0 = <&bt_cfg_en>;
		status = "okay";
	};
	wlan {
		compatible = "nexell,brcm-wlan";
		wlan_reg_on = <&gpio_a 12 0>;
		wlan_host_wake = <&gpio_a 13 1>;
		status = "okay";
    };
};



&dw_mmc_0 {
	num-slots = <1>;
	bus-width = <4>;
	cap-mmc-highspeed;
	broken-cd;
	non-removable;
	clock-frequency = <100000000>;
	card-detect-delay = <200>;
	disable-wp;
	nexell,drive_dly = <0x0>;
	nexell,drive_shift = <0x02>;
	nexell,sample_dly = <0x00>;
	nexell,sample_shift = <0x01>;
	status = "okay";
};

&dw_mmc_2 {
	num-slots = <1>;
	bus-width = <4>;
	cap-sd-highspeed;
	cd-type-external;
	broken-cd;
	pm-ignore-notify;
	keep-power-in-suspend;
	powered-resumed-nonremovable-card;
	cd-type = <2>;
	clock-frequency = <100000000>;
	nexell,drive_dly = <0x0>;
	nexell,drive_shift = <0x02>;
	nexell,sample_dly = <0x00>;
	nexell,sample_shift = <0x01>;

	pinctrl-names = "default";
	pinctrl-0 = <&sdmmc2_cclk &sdmmc2_cmd &sdmmc2_bus4
			&cfg_wlanen &cfg_wlanhostwake>;

	status = "okay";
};
&dw_mmc_1 {
	num-slots = <1>;
	bus-width = <4>;
	cap-sd-highspeed;
	clock-frequency = <100000000>;
	card-detect-delay = <200>;
	disable-wp;
	cd-gpios = <&gpio_c 24 0>;
	cd-inverted;
	nexell,drive_dly = <0x0>;
	nexell,drive_shift = <0x02>;
	nexell,sample_dly = <0x00>;
	nexell,sample_shift = <0x01>;
	status = "okay";
};

&i2s0_bus {
	i2s0_mclk: i2s0-mclk {
		nexell,pin-strength = <0>;
	};
	i2s0_bclk: i2s0-bclk {
		nexell,pin-strength = <0>;
	};
};

&pwm {
	pinctrl-names = "default";
	pinctrl-0 = <&pwm0_pin &pwm1_pin>;
	samsung,pwm-outputs = <0>, <1>;
	status = "okay";
};

