#include <dt-bindings/interrupt-controller/irq.h>
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include "nxp4330.dtsi"

#define PMIC_PDATA_INIT(_id, _rname, _minuv,			\
			_maxuv, _init_uv, _init_enable,		\
			_set_init_uV, _slp_slots)		\
			regulator-name = _rname;		\
			regulator-min-microvolt = <_minuv>;	\
			regulator-max-microvolt = <_maxuv>;	\
			nx,id = <_id>;				\
			nx,init_enable = <_init_enable>;	\
			nx,init_uV = <_init_uv>;		\
			nx,set_init_uV = <_set_init_uV>;	\
			nx,sleep_slots = <_slp_slots>;

/ {
	memory {
		device_type = "memory";
		reg = <0x71000000 0x40000000>;
	};

	psci {
		compatible = "arm,psci-0.2";
		method = "smc";
	};

	aliases {
		i2c3 = &i2c_gpio3;
		i2c4 = &i2c_gpio4;
		i2c5 = &i2c_gpio5;
		i2c6 = &i2c_gpio6;
	};

	soc {
		#include "s5p4418-pinctrl.dtsi"
		clocks {
			uart3:uart@c00ab000 { clock-frequency = <147500000>; };
			uart1:uart@c00a8000 { clock-frequency = <100000000>; };
			uart0:uart@c00a9000 { clock-frequency = <147500000>; };
			dummy_clk: dummy_clk {
				#clock-cells = <0>;
				clock-output-names = "dummy_clk";
				compatible = "fixed-clock";
				clock-frequency = <0>;
			};
			pwm0:pwm0@c00ba000 {
				clk-input = <I_PLL2>;
				clock-frequency = <10000000>;
			};

			pwm1:pwm1@c00ba000 {
				clk-input = <I_PLL0>;
				clock-frequency = <80000000>;
			};
		};

		pwm:pwm@c0018000 {
/*
			clocks =  <&pwm0>, <&pwm1>;
			clock-names = "pwm-tclk0", "pwm-tclk1";
*/
			clock-names = "timers", "pwm-tclk0", "pwm-tclk1";
			clocks =  <&pclk>, <&pwm0>, <&pwm1>;
			pinctrl-names = "default";
			pinctrl-0 = <&pwm0_pin &pwm1_pin>;
			samsung,pwm-outputs = <0>, <1>;
			status = "okay";
		};

		pinctrl@C0010000 {
			/*
			i2s1_bus:i2s1 {
				/delete-node/ i2s1-mclk;
			};

			i2s2_bus:i2s2 {
				/delete-node/ i2s2-mclk;
			};
			*/
			/delete-node/ spdiftx_pin;
            /delete-node/ i2s1_bus;
			/delete-node/ dp_rgb_vclk;
			/delete-node/ dp_rgb_vsync;
			/delete-node/ dp_rgb_hsync;
			/delete-node/ dp_rgb_de;
			/delete-node/ dp_rgb_B;
			/delete-node/ dp_rgb_G;
			/delete-node/ dp_rgb_R;
			/delete-node/ mpegts_cap0_data_clk_serial;
			/delete-node/ mpegts_cap0_sync;
			/delete-node/ mpegts_cap1_data_clk_parallel;
			/delete-node/ mpegts_cap1_data_clk_serial;
            i2s1_bus:i2s1 {
                nexell,pins = "gpioa-30", "gpiob-0",
                    "gpiob-6", "gpiob-9";
                nexell,pin-function = <NX_PIN_FUNC3>;
                nexell,pin-pull = <NX_PIN_PULL_DOWN>;
                nexell,pin-strength = <NX_PIN_STR0>;
            };

			bt_cfg_en:bt_cfg_en {
				nexell,pins = "gpioa-9";
				nexell,pin-function = <0>;
				nexell,pin-pull = <2>;
				nexell,pin-strength = <0>;
			};
			cfg_wlanen: cfg-wlanen {
				nexell,pins ="gpioa-12";
				nexell,pin-function = <0>;
				nexell,pin-pull = <2>;
				nexell,pin-strength = <0>;
			};
			cfg_wlanhostwake: cfg-wlanhostwake {
				nexell,pins = "gpioa-13";
				nexell,pin-function = <0>;
				nexell,pin-pull = <1>;
				nexell,pin-strength = <0>;
			};
		};

		l2:l2-cache-controller@cf000000 {
			arm,prefetch-offset = <7>;
			prefetch-data = <1>;
			prefetch-instr = <1>;
			arm,tag-latency = <3 4 3>;
			arm,data-latency = <3 4 3>;
		};
		serial0:serial@c00a1000 {
			status ="okay";
		};

		serial1:serial@c00a0000 {
			dmas = <&pl08xdma0 0 0>, <&pl08xdma0 1 0>;
			dma-names = "tx", "rx";
			pinctrl-0 = <&serial1_pin>, <&serial1_flow_cts>,
						<&serial1_flow_rts>;
			status ="okay";
		};

		serial3:serial@c00a3000 {
			status ="okay";
		};

		amba {
			pl08xdma0:pl08xdma@c0000000 {
				use_isr;
			};

			pl08xdma1:pl08xdma@c0001000 {
				use_isr;
			};
		};

		watchdog@c0019000 {
			status = "okay";
		};

		rtc@c0010c00 {
			status = "okay";
		};

		nexell_usbphy: nexell-usbphy@c0012000 {
			status = "okay";
		};

		ehci@c0030000 {
			status = "okay";
			port@0 {
				status = "okay";
			};
		};

		ohci@c0020000 {
			status = "okay";
			port@0 {
				status = "okay";
			};
		};

		dwc2otg@c0040000 {
			gpios = <&alive_0 5 0>;
			status = "okay";
		};

		i2s_0:i2s@c0055000 {
			#sound-dai-cells = <1>;
			master-mode = <1>;
			mclk-in = <0>;
			trans-mode = <0>;
			frame-bit = <32>;
			sample-rate = <48000>;
			pre-supply-mclk = <1>;
			status = "okay";
		};

		i2s_1:i2s@c0056000 {
			#sound-dai-cells = <1>;
			master-mode = <0>;
			mclk-in = <1>;
			trans-mode = <0>;
			frame-bit = <32>;
			sample-rate = <48000>;
			pre-supply-mclk = <0>;
			status = "okay";
		};

		i2s_2:i2s@c0057000 {
			#sound-dai-cells = <1>;
			master-mode = <0>;
			mclk-in = <1>;
			trans-mode = <0>;
			frame-bit = <32>;
			sample-rate = <48000>;
			pre-supply-mclk = <0>;
			status = "okay";
		};

		spdif_tx: spdiftx@c0059000 {
			#sound-dai-cells = <1>;
			pcm-bit = <16>;
			sample_rate = <48000>;
			status = "okay";
		};

		snd_spi_2:snd_spi@c005f000 {
			data-bit = <16>;
			clock-frequency = <100000000>;
			clock-invert = <1>;
			clock-phase = <1>;
			status = "okay";
		};

        pinctrl@C0010000 {
            key_volup:key_volup {
                nexell,pins = "gpiod-29";
                nexell,pin-function = <0>;
                nexell,pin-pull = <1>;
                nexell,pin-strength = <0>;
            };

            key_voldown:key_voldown {
                nexell,pins = "gpiod-30";
                nexell,pin-function = <0>;
                nexell,pin-pull = <1>;
                nexell,pin-strength = <0>;
            };

            key_func1:key_func1 {
                nexell,pins = "gpioe-0";
                nexell,pin-function = <0>;
                nexell,pin-pull = <1>;
                nexell,pin-strength = <0>;
            };

            key_func2:key_func2 {
                nexell,pins = "gpioe-1";
                nexell,pin-function = <0>;
                nexell,pin-pull = <1>;
                nexell,pin-strength = <0>;
            };

            key_mute:key_mute {
                nexell,pins = "gpiod-31";
                nexell,pin-function = <0>;
                nexell,pin-pull = <1>;
                nexell,pin-strength = <0>;
            };
        };
		i2c_1:i2c@c00a5000 {
			#address-cells = <1>;
			#size-cells = <0>;
			status = "okay";

        	tas5756m:tas5756m@4E {
				#sound-dai-cells = <0>;
            	compatible = "ti,tas5756m";
	            status = "okay";
            	reg = <0x4E>;
            	tas5756m,mute_gpio = <&gpio_c 29 0>;
				tas5756m,spk_en_gpio = <&gpio_c 30 0>;
	        };
		};
        i2c_2:i2c@c00a6000 {
            status = "okay";
            #address-cells = <1>;
            #size-cells = <0>;

            goodix@5d {
                compatible = "goodix,gt911";
                reg = <0x5d>;
                interrupt-parent = <&gpio_a>;
                interrupts = <15 IRQ_TYPE_EDGE_FALLING>;
                irq-gpios = <&gpio_a 15 0>;
                reset-gpios = <&gpio_a 14 0>;
            };
        };

		i2c_gpio3: i2c@3 {
			compatible = "i2c-gpio";
			gpios = <&gpio_d 3 0
				&gpio_d 2 0
				>;
			#address-cells = <1>;
			#size-cells = <0>;
			status = "okay";

			nxe2000@32 {
				compatible = "nexell,nxe2000";
				reg = <0x32>;
				interrupt-parent = <&alive_0>;
				interrupts = <0x4 IRQ_TYPE_EDGE_FALLING>;

				regulators {
					VCC_DCDC0:
						dcdc1	{ PMIC_PDATA_INIT( 0,
							"nxe2000_dcdc1",
							900000, 1500000,
							1100000, 1, 0, 0x4)
							regulator-always-on;
							regulator-boot-on;};
					VCC_DCDC1:
						dcdc2	{ PMIC_PDATA_INIT( 1,
							"nxe2000_dcdc2",
							1000000, 1500000,
							1200000, 1, 0, 0x4)
							regulator-always-on;
							regulator-boot-on;};
					VCC_DCDC2:
						dcdc3	{ PMIC_PDATA_INIT( 2,
							"nxe2000_dcdc3",
							1000000, 3500000,
							3300000, 1, 1, 0x0)
							regulator-always-on;
							regulator-boot-on;};
					VCC_DCDC3:
						dcdc4	{ PMIC_PDATA_INIT( 3,
							"nxe2000_dcdc4",
							1000000, 2000000,
							1500000, 1, 0, 0xF)
							regulator-always-on;
							regulator-boot-on;};
                    VCC_DCDC4:
                        dcdc5   { PMIC_PDATA_INIT( 4,
                            "nxe2000_dcdc5",
                            1000000, 2000000,
                            1500000, 1, 0, 0x4)
                            regulator-always-on;
                            regulator-boot-on;};
							/*
					VCC_DCDC4:
						dcdc5	{ PMIC_PDATA_INIT( 4,
							"nxe2000_dcdc5",
							1000000, 2000000,
							1500000, 0, 0, 0x4)
							regulator-always-on;
							regulator-boot-on;};
							*/
					VCC_LDO0:
						ldo1	{ PMIC_PDATA_INIT( 5,
							"nxe2000_ldo1",
							1000000, 3500000,
							1800000, 0, 1, 0x0)
							regulator-boot-on;};
					VCC_LDO1:
						ldo2	{ PMIC_PDATA_INIT( 6,
							"nxe2000_ldo2",
							1000000, 3500000,
							1800000, 0, 1, 0x0)
							regulator-boot-on;};
					VCC_LDO2:
						ldo3	{ PMIC_PDATA_INIT( 7,
							"nxe2000_ldo3",
							1000000, 3500000,
							1800000, 1, 1, 0x2)
							regulator-always-on;
							regulator-boot-on;};
					VCC_LDO3:
						ldo4	{ PMIC_PDATA_INIT( 8,
							"nxe2000_ldo4",
							1000000, 3500000,
							1800000, 1, 1, 0x2)
							regulator-always-on;
							regulator-boot-on;};
					VCC_LDO4:
						ldo5	{ PMIC_PDATA_INIT( 9,
							"nxe2000_ldo5",
							1000000, 3500000,
							3300000, 0, 1, 0x0)
							regulator-boot-on;};
					VCC_LDO5:
						ldo6	{ PMIC_PDATA_INIT(10,
							"nxe2000_ldo6",
							1000000, 3500000,
							3300000, 1, 1, 0xF)
							regulator-always-on;
							regulator-boot-on;};
					VCC_LDO6:
						ldo7	{ PMIC_PDATA_INIT(11,
							"nxe2000_ldo7",
							1000000, 3500000,
							2800000, 0, 1, 0x1)
							regulator-always-on;
							regulator-boot-on;};
					VCC_LDO7:
						ldo8	{ PMIC_PDATA_INIT(12,
							"nxe2000_ldo8",
							1000000, 3500000,
							3300000, 0, 1, 0x0)
							regulator-boot-on;};
					VCC_LDO8:
						ldo9	{ PMIC_PDATA_INIT(13,
							"nxe2000_ldo9",
							1000000, 3500000,
							3300000, 0, 1, 0x0)
							regulator-boot-on;};
					VCC_LDO9:
						ldo10	{ PMIC_PDATA_INIT(14,
							"nxe2000_ldo10",
							1000000, 3500000,
							1000000, 1, 1, 0x0)
							regulator-always-on;
							regulator-boot-on;};
					VCC_RTC0:
						ldortc1	{ PMIC_PDATA_INIT(15,
							"nxe2000_rtc1",
							1700000, 3500000,
							1800000, 1, 1, 0xF)
							regulator-always-on;
							regulator-boot-on;};
					VCC_RTC1:
						ldortc2	{ PMIC_PDATA_INIT(16,
							"nxe2000_rtc2",
							1000000, 3500000,
							1000000, 1, 1, 0xF)
							regulator-always-on;
							regulator-boot-on;};
				}; 
			}; 
		}; 
        i2c_gpio4: i2c@4 {
            compatible = "i2c-gpio";
            gpios = <&gpio_a 26 0        /* SDA */
                &gpio_a 27 0     /* SCL */
                >;
            #address-cells = <1>;
            #size-cells = <0>;
            status = "okay";

        	led_lp55231_1@32 {
				compatible = "ti,lp55231";
		        reg = <0x32>;
		        //pinctrl-names = "led_en";
		        //pinctrl-0 = <&led_en>;
		        enable-gpio = <&gpio_e 3 0x1>;
	    	    clock-mode = <2>;
		        chan0 {
		            chan-name = "d1";
		            led-cur = <0x14>;
		            max-cur = <0x20>;
		        };
		        chan1 {
		            chan-name = "d2";
		            led-cur = <0x14>;
		            max-cur = <0x20>;
		        };
		        chan2 {
		            chan-name = "d3";
		            led-cur = <0x14>;
		            max-cur = <0x20>;
		        };
		        chan3 {
		            chan-name = "d4";
		            led-cur = <0x14>;
		            max-cur = <0x20>;
		        };
		        chan4 {
		            chan-name = "d5";
		            led-cur = <0x14>;
		            max-cur = <0x20>;
		        };
	            chan5 {
		            chan-name = "d6";
		            led-cur = <0x14>;
		            max-cur = <0x20>;
		        };
		        chan6 {
		            chan-name = "d7";
		            led-cur = <0x14>;
		            max-cur = <0x20>;
		        };
		        chan7 {
		            chan-name = "d8";
		            led-cur = <0x14>;
		            max-cur = <0x20>;
	    	    };
		        chan8 {
		            chan-name = "d9";
		            led-cur = <0x14>;
		            max-cur = <0x20>;
	    	    };
			};
        }; /* i2c_gpio4 */
        i2c_gpio5: i2c@5 {
            compatible = "i2c-gpio";
            gpios = <&gpio_e 5 0        /* SDA */
                &gpio_e 6 0     /* SCL */
                >;
            #address-cells = <1>;
            #size-cells = <0>;
            status = "okay";

            led_lp55231_1@32 {
	            compatible = "ti,lp55231";
	            reg = <0x33>;
	            //pinctrl-names = "led_en";
	            //pinctrl-0 = <&led_en>;
	            enable-gpio = <&gpio_e 3 0x1>;
	            clock-mode = <2>;

	            chan0 {
	                chan-name = "d10";
	                led-cur = <0x14>;
	                max-cur = <0x20>;
	            };
	            chan1 {
	                chan-name = "d11";
	                led-cur = <0x14>;
	                max-cur = <0x20>;
	            };
	            chan2 {
	                chan-name = "d12";
	                led-cur = <0x14>;
	                max-cur = <0x20>;
	            };
	            chan3 {
	                chan-name = "d13";
	                led-cur = <0x14>;
	                max-cur = <0x20>;
	            };
	            chan4 {
	                chan-name = "d14";
	                led-cur = <0x14>;
	                max-cur = <0x20>;
	            };
	            chan5 {
	                chan-name = "d15";
	                led-cur = <0x14>;
	                max-cur = <0x20>;
	            };
	            chan6 {
	                chan-name = "d16";
	                led-cur = <0x14>;
	                max-cur = <0x20>;
	            };
	            chan7 {
	                chan-name = "d17";
	                led-cur = <0x14>;
	                max-cur = <0x20>;
	            };
	            chan8 {
	                chan-name = "d18";
	                led-cur = <0x14>;
	                max-cur = <0x20>;
	            };
			};
		};  /* i2c_gpio5 */
	    i2c_gpio6: i2c@6 {
	        compatible = "i2c-gpio";
	        gpios = <&gpio_c 24 0 /* sda */
	            &gpio_c 14 0 /* scl */
	            >;
	        i2c-gpio,delay-us = <100>;
	        #address-cells = <1>;
	        #size-cells = <0>;

	        usb2514: usb2514@2c {
	            compatible = "smsc,usb2514";
	            reg = <0x2c>;
	            reset-gpios = <&gpio_c 15 1>;
	            initial-mode = <1>;
	        };
	    };	/* i2c_gpio6 */
	}; /*** soc ***/

