&i2c_gpio6 {
	compatible = "i2c-gpio";
	gpios=<&gpio_e 1 0	/* SDA */
	      &gpio_e 0 0>;	/* SCL */
	status = "okay";

	goodix@5d {
		compatible = "goodix,gt9271";
		reg = <0x5d>;
		interrupt-parent = <&gpio_e>;
		interrupts = <3 IRQ_TYPE_EDGE_FALLING>;
		irq-gpios = <&gpio_e 3 0>;
		reset-gpios = <&gpio_c 8 0>;
	};
};

