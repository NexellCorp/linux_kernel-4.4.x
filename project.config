[access]
	inheritFrom = All-Projects
[project]
	description = linux kernel version 4.4.x repository
[access "refs/*"]
	owner = group user/swpark
[access "refs/for/refs/*"]
	read = group user/swpark
	push = group user/swpark
	label-Code-Review = -2..+2 group user/swpark
	label-Verified = -1..+1 group user/swpark
	rebase = group user/swpark
	removeReviewer = group user/swpark
	submit = group user/swpark
[access "refs/heads/*"]
	read = group user/swpark
	create = group user/swpark
	push = +force group user/swpark
	pushMerge = group user/swpark
[access "refs/tags/*"]
	push = group user/swpark
	push = +force group user/swpark
[receive]
	requireSignedOffBy = true
	requireChangeId = true
[submit]
	action = rebase always
	mergeContent = false
